{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "jest": true,
    "browser": true,
    "cypress/globals": true
  },
  "plugins": [
    "react",
    "flow",
    "graphql",
    "jsx-a11y",
    "promise",
    "standard",
    "cypress"
  ]
}
